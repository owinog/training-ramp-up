{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/rampup-backend/src",
  "monorepo": true,
  "root": "apps/rampup-backend",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/rampup-backend/tsconfig.app.json"
  },
  "projects": {
    "rampup-backend": {
      "type": "application",
      "root": "apps/rampup-backend",
      "entryFile": "main",
      "sourceRoot": "apps/rampup-backend/src",
      "compilerOptions": {
        "tsConfigPath": "apps/rampup-backend/tsconfig.app.json"
      }
    },
    "student-actions": {
      "type": "application",
      "root": "apps/student-actions",
      "entryFile": "main",
      "sourceRoot": "apps/student-actions/src",
      "compilerOptions": {
        "tsConfigPath": "apps/student-actions/tsconfig.app.json"
      }
    },
    "file-upload-service": {
      "type": "application",
      "root": "apps/file-upload-service",
      "entryFile": "main",
      "sourceRoot": "apps/file-upload-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/file-upload-service/tsconfig.app.json"
      }
    },
    "file-processing": {
      "type": "application",
      "root": "apps/file-processing",
      "entryFile": "main",
      "sourceRoot": "apps/file-processing/src",
      "compilerOptions": {
        "tsConfigPath": "apps/file-processing/tsconfig.app.json"
      }
    },
    "notifications": {
      "type": "application",
      "root": "apps/notifications",
      "entryFile": "main",
      "sourceRoot": "apps/notifications/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notifications/tsconfig.app.json"
      }
    },
    "authentication": {
      "type": "application",
      "root": "apps/authentication",
      "entryFile": "main",
      "sourceRoot": "apps/authentication/src",
      "compilerOptions": {
        "tsConfigPath": "apps/authentication/tsconfig.app.json"
      }
    }
  }
}